public class LevelMain 
{
	public static void main ( String [] args )
	{
		Level level = Level.MEDIUM ;
		
		switch ( level )
		{
		case LOW :
			System.out.println( "Low Level" );
			break ;
			
		case MEDIUM :
			System.out.println( "Medium Level" );
			break ;
			
		case HIGH :
			System.out.println( "Hard Level" );
			break ;
		}
	}
}
