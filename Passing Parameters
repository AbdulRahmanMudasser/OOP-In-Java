package methods;

public class PassingParametersToMethod 
{

  // Passing Parameter

	static void mymethod ( String fullname )
	{
		System.out.println( fullname + " Hussain" );
	}
	
	public static void main ( String [] args )
	{
  
    // Calling Method
  
		mymethod( "Mudasser" );		// Argument
		mymethod( "Mubashir" );		// Argument
		mymethod( "Muzzamil" );		// Argument
	}
}
